<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="config.mybatis.mapper.oracle.operator">
<insert id="insertOperAtor" parameterType="OperAtorVO">
<selectKey keyProperty="opNo" resultType="int" order="BEFORE">
	select operator_seq.nextval from dual
</selectKey>
insert into operator(op_no,f_no,k_no,op_showflag)
values(#{opNo},#{fNo},#{kNo},#{opShowflag})
</insert>


</mapper>








